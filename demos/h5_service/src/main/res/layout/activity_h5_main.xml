<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".H5MainActivity">

    <Button
        android:id="@+id/updateOfflineResourcesBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_update_offline_resource"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/openDevelopActivityBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_open_develop_activity"
        android:textAllCaps="false" />


    <Button
        android:id="@+id/openExampleOnlineActivityBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_open_example_online_activity"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/openExampleOfflineActivityBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_open_example_offline_activity"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/exampleTTWebViewBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_open_example_ttwebview_activity"
        android:textAllCaps="false"/>

</LinearLayout>